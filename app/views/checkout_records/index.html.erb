<p id="notice"><%= notice %></p>

<h1>Checkout Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @checkout_records.each do |checkout_record| %>
      <tr>
        <td><%= link_to 'Show', checkout_record %></td>
        <td><%= link_to 'Edit', edit_checkout_record_path(checkout_record) %></td>
        <td><%= link_to 'Destroy', checkout_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Checkout Record', new_checkout_record_path %>
